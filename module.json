{
  "name":"DS100-Joris",
  "type":"OSC",
  "path":"Custom",
  
  "version":"1.0.0",
  "description":"My module description",
  "url":"https://github.com/tommag/Sample-Chataigne-module",
  "downloadURL":"https://github.com/tommag/Sample-Chataigne-module/archive/master.zip",

  "hasInput":true,
  "hasOutput":true, 

  "hideDefaultCommands":false,
  
  "defaults":
  {	
    "autoAdd":false,
    "oscInput":
    {
      "localPort":40016
    }
  },

  "parameters":
  {
    "DiGiCo IP":
  {
    "type":"String",
    "default":"127.0.0.1"
  },
    "DiGiCo RX Port":
    {
      "type":"Integer",
      "default":9000
    }
  },

  "hideDefaultParameters": [ "autoAdd"],

  "scripts": 
  [
    "moduleScript.js"
  ],

  "values":
  {
  },

  "commands":
  {
    
    "Custom command":
    {
      "menu":"",  
      "callback":"customCmd",
      "parameters":
      {
        "Value":
        {
          "type":"Integer",
          "min":0,
          "max":100,
          "default":0
        }
      }
    },
    "reverbSendGain":
		{
			"menu":"Sound Objects",
			"callback":"reverbSendGain",
			"parameters":
			{
				"Sound Object #":
				{
					"type":"Integer",
					"min":1,
					"max":64
				},
				"Send gain dB":
				{
					"type":"Float",
					"min":-120,
					"max":24
				}
			}
		}
  }
}
