{
  "name":"DS100-Joris",
  "type":"OSC",
  "path":"Custom",
  
  "version":"1.0.0",
  "description":"d&b DS100 implementation via OSC with DiGiCo data passthrough",
  "url":"https://github.com/JorisMachon/chataigne-custom-ds100",
  "downloadURL":"",

  "hasInput":true,
  "hasOutput":true, 

  "hideDefaultCommands":false,
  
  "defaults":
  {	
    "autoAdd":false,
    "oscInput":
    {
      "localPort":40016
    }
  },

  "parameters":
  {
    "DiGiCo IP":
  {
    "type":"String",
    "default":"127.0.0.1"
  },
    "DiGiCo RX Port":
    {
      "type":"Integer",
      "default":9000
    }
  },

  "hideDefaultParameters": [ "autoAdd"],

  "scripts": 
  [
    "moduleScript.js"
  ],

  "values":
  {
  },

  "commands":
  {
    "reverbSendGain":
		{
			"menu":"Sound Objects",
			"callback":"reverbSendGain",
			"parameters":
			{
				"Sound Object #":
				{
					"type":"Integer",
					"min":1,
					"max":64
				},
				"Send gain dB":
				{
					"type":"Float",
					"min":-120,
					"max":24
				}
			}
		}
  }
}
